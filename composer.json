{
    "name": "ajtarragona/skoolpoint-client",
    "description": "Client Laravel de la API Rest de Skoolpoint",
    "keywords" : ["laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Txomin Medrano Martorell",
            "email": "tmedrano@tarragona.cat"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3|^8.0",
        "laravel/framework": ">=6.0",
        "guzzlehttp/guzzle": ">=6.3"
    },
    "autoload": {
        "psr-4": {
            "Ajtarragona\\Skoolpoint\\": "./src"
        }
    },
    
    "extra": {
        "laravel": {
            "providers": [
                "Ajtarragona\\Skoolpoint\\SkoolpointServiceProvider"
            ],
            "aliases": {
                "Skoolpoint" : "Ajtarragona\\Skoolpoint\\Facades\\Skoolpoint"
            }
        }
    }
}
